package main

import (
	"fmt"
	"strings"
)

func main() {
	fmt.Println(strings.Contains("Go programming language", "Go"))
	fmt.Println(strings.Count("Go programming language", "g"))
	fmt.Println(strings.Index("Go programming language", "o"))
	fmt.Println(strings.LastIndex("Go programming language", "l"))
	fmt.Println(strings.Index("Go programming language", "a"))
	fmt.Println(strings.Count("Go programming language", "n"))
	fmt.Println(strings.EqualFold("Go programming language", "g"))
}
