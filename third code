package main

import "fmt"

func main() {
	/*
		num1 := 122
		str1 := string(num1)

		fmt.Printf("%v %T\n", num1, num1)
		fmt.Println()
		fmt.Printf("%v %T\n", str1, str1)
	*/
	var (
		a       = 55.675
		b       = 78.987
		c       = 999
		d       = complex(3, 8)
		average = (complex(a, 0) + complex(b, 0) + complex(float64(c), 0) + d) / 4
	)
	fmt.Printf("%.3f , %.3f , %d , %v The average value of their number is: %.10v ", a, b, c, d, average)
}
